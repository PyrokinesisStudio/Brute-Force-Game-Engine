<?xml version="1.0" encoding="utf-8"?>

<ObjectConfigs>

  <ObjectConfig name="Invader">
    <Module name="Root">
      <Concepts>InvaderConcept</Concepts>
	  <CustomValues>
		<PV name="Density">50.0</PV>
		<PV name="PhysicalMesh">Packer.mesh</PV>
		<PV name="VisualMesh">Packer.mesh</PV>
	  </CustomValues>
	  </Module>
  </ObjectConfig>

  <ObjectConfig name="Ship">
    <Module name="Root">
      <Concepts>ShipConcept</Concepts>
	  <CustomValues>
		<PV name="Density">10000.0</PV>
		<PV name="PhysicalMesh">Arrow.mesh</PV>
		<PV name="VisualMesh">Arrow.mesh</PV>
	  </CustomValues>
    </Module>
  </ObjectConfig>

  <ObjectConfig name="Projectile">
    <Module name="Root">
      <Concepts>ProjectileConcept</Concepts>
    </Module>
  </ObjectConfig>

  <ObjectConfig name="AutoProjectile">
    <Module name="Root">
      <Concepts>AutoProjectileConcept</Concepts>
    </Module>
  </ObjectConfig>

  <ObjectConfig name="Powerup">
    <Module name="Root">
      <Concepts>PowerupConcept</Concepts>
 	  <CustomValues>
		<PV name="CollisionMode">CM_Ghost</PV>
		<PV name="Density">5.0</PV>
	  </CustomValues>
   </Module>
  </ObjectConfig>

</ObjectConfigs>
